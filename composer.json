    "keywords": [
        "waavi",
        "value object",
        "eloquent",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com",
            "homepage": "http://waavi.com"
        }
    ],
    "require": {
        "illuminate/support": "5.1.x",
        "illuminate/cache": "5.1.x",
        "illuminate/http": "5.1.x"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4",
        "orchestra/testbench": "3.1.x",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\ValueObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\ValueObjects\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
